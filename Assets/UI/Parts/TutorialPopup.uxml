            <ui:Button text="&lt;" parse-escape-sequences="true" display-tooltip-when-elided="true" name="leftButton" style="font-size: 25px; background-color: rgba(188, 188, 188, 0); border-left-color: rgba(149, 149, 149, 0); border-right-color: rgba(149, 149, 149, 0); border-top-color: rgba(149, 149, 149, 0); border-bottom-color: rgba(149, 149, 149, 0);" />
            <ui:VisualElement style="flex-grow: 1;">
                <ui:VisualElement name="firstTutorial" style="position: absolute; width: 100%; height: 100%; display: none;">
                    <ui:Label tabindex="-1" text="ゲーム開始！" parse-escape-sequences="true" display-tooltip-when-elided="true" style="-unity-text-align: upper-center; -unity-font-style: bold; font-size: 24px;" />
                    <ui:VisualElement style="flex-grow: 1; background-image: url(&quot;project://database/Assets/Images/Tutorial/gameStartTutorial.png?fileID=21300000&amp;guid=9f1610bcc06e6514592d0b3b6c95d689&amp;type=3#gameStartTutorial&quot;); -unity-background-scale-mode: scale-to-fit;" />
                    <ui:Label tabindex="-1" parse-escape-sequences="true" display-tooltip-when-elided="true" text="二人で協力プレイ！&#10;ソルジャー画面のIDをコマンダー画面で「msc &quot;id&quot;」で入力" style="-unity-text-align: upper-center;" />
                </ui:VisualElement>
                <ui:VisualElement name="secondTutorial" style="position: absolute; width: 100%; height: 100%; display: none;">
                    <ui:Label tabindex="-1" text="タイルをすべてめくろう！" parse-escape-sequences="true" display-tooltip-when-elided="true" style="-unity-text-align: upper-center; -unity-font-style: bold; font-size: 24px;" />
                    <ui:VisualElement style="flex-grow: 1; background-image: url(&quot;project://database/Assets/Images/Tutorial/mapTutorial.png?fileID=21300000&amp;guid=9a714c096b89a574d899ba3b5f3ceb82&amp;type=3#mapTutorial&quot;); -unity-background-scale-mode: scale-to-fit;" />
                    <ui:Label tabindex="-1" parse-escape-sequences="true" display-tooltip-when-elided="true" text="周囲に何もないと複数枚のタイルが同時にめくれる&#10;うまくコミュニケーションをとってすべてのタイルをめくろう" style="-unity-text-align: upper-center;" />
                </ui:VisualElement>
                <ui:VisualElement name="thirdTutorial" style="position: absolute; width: 100%; height: 100%; display: none;">
                    <ui:Label tabindex="-1" text="アイテムの活用" parse-escape-sequences="true" display-tooltip-when-elided="true" style="-unity-text-align: upper-center; -unity-font-style: bold; font-size: 24px;" />
                    <ui:VisualElement style="flex-grow: 1; background-image: url(&quot;project://database/Assets/Images/Tutorial/itemTutorial.png?fileID=21300000&amp;guid=dccb93ff31ef86b4c9424a6c2a89ed0e&amp;type=3#itemTutorial&quot;); -unity-background-scale-mode: scale-to-fit;" />
                    <ui:Label tabindex="-1" parse-escape-sequences="true" display-tooltip-when-elided="true" text="タイルをめくるとアイテムゲット！&#10;うまく使ってゲームを有利に進めよう" style="-unity-text-align: upper-center;" />
