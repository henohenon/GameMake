<ui:UXML xmlns:ui="UnityEngine.UIElements" xmlns:uie="UnityEditor.UIElements" xsi="http://www.w3.org/2001/XMLSchema-instance" engine="UnityEngine.UIElements" editor="UnityEditor.UIElements" noNamespaceSchemaLocation="../../UIElementsSchema/UIElements.xsd" editor-extension-mode="False">
    <ui:VisualElement style="width: 100%; height: 100%; padding-top: 30px; padding-right: 30px; padding-bottom: 30px; padding-left: 30px;">
        <ui:VisualElement style="flex-grow: 1;">
            <ui:VisualElement name="firstTutorial" style="position: absolute; width: 100%; height: 100%; display: flex;">
                <ui:Label tabindex="-1" text="Game  Start" parse-escape-sequences="true" display-tooltip-when-elided="true" style="font-size: 36px; -unity-text-align: upper-center;" />
                <ui:VisualElement style="flex-grow: 1; background-image: url(&quot;project://database/Assets/Images/Tutorial/GameStart.png?fileID=2800000&amp;guid=ac63d16dbe901ce4488dca7c7baecfb2&amp;type=3#GameStart&quot;); -unity-background-scale-mode: scale-to-fit;" />
                <ui:Label tabindex="-1" parse-escape-sequences="true" display-tooltip-when-elided="true" text="二人で協力プレイ！&#10;ソルジャー画面のIDをコマンダー画面で「msc &quot;id&quot;」で入力" style="-unity-text-align: upper-center; font-size: 16px;" />
            </ui:VisualElement>
            <ui:VisualElement name="secondTutorial" style="position: absolute; width: 100%; height: 100%; display: none;">
                <ui:Label tabindex="-1" text="Open All Tiles" parse-escape-sequences="true" display-tooltip-when-elided="true" style="font-size: 36px; -unity-text-align: upper-center;" />
                <ui:VisualElement style="flex-grow: 1; background-image: url(&quot;project://database/Assets/Images/Tutorial/OpenTiles.png?fileID=21300000&amp;guid=f5eeec11f75d6354e9a45c6358983b0b&amp;type=3#OpenTiles&quot;); -unity-background-scale-mode: scale-to-fit;" />
                <ui:Label tabindex="-1" parse-escape-sequences="true" display-tooltip-when-elided="true" text="周囲に何もないと複数枚のタイルが同時にめくれる&#10;うまくコミュニケーションをとってすべてのタイルをめくろう" style="-unity-text-align: upper-center; font-size: 16px;" />
            </ui:VisualElement>
            <ui:VisualElement name="thirdTutorial" style="position: absolute; width: 100%; height: 100%; display: none;">
                <ui:Label tabindex="-1" text="Use Items" parse-escape-sequences="true" display-tooltip-when-elided="true" style="font-size: 36px; -unity-text-align: upper-center;" />
                <ui:VisualElement style="flex-grow: 1; background-image: url(&quot;project://database/Assets/Images/Tutorial/UseItem.png?fileID=21300000&amp;guid=e3d66772a63e0584ca4cae03c2246e4d&amp;type=3#UseItem&quot;); -unity-background-scale-mode: scale-to-fit;" />
                <ui:Label tabindex="-1" parse-escape-sequences="true" display-tooltip-when-elided="true" text="タイルをめくるとアイテムゲット！&#10;うまく使ってゲームを有利に進めよう" style="-unity-text-align: upper-center; font-size: 16px;" />
